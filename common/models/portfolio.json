{
  "name": "Portfolio",
  "description": "Member Portfolio model",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "scope": {
    "include": [
      "PortfolioCoins"
    ]
  },
  "properties": {
    "active": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "Coins": {
      "type": "hasMany",
      "model": "Coin",
      "through": "PortfolioCoin"
    },
    "Member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "ownerId"
    },
    "PortfolioCoins": {
      "type": "hasMany",
      "model": "PortfolioCoin",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}

{
  "name": "Member",
  "description": "The Member model extends User",
  "base": "User",
  "idInjection": true,
  "emailVerificatonRequired": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "scope": {
    "include": [
      "Portfolio",
      "Role"
    ]
  },
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "dob": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "currency": {
      "type": "string"
    },
    "newsletter": {
      "type": "boolean"
    },
    "reportFrequency": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "Portfolio": {
      "type": "hasMany",
      "model": "Portfolio",
      "foreignKey": "ownerId"
    },
    "PortfolioCoins": {
      "type": "hasMany",
      "model": "PortfolioCoin",
      "through": "Portfolio"
    },
    "Role": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "RoleMappping": {
      "type": "hasMany",
      "model": "RoleMappping"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "findById",
        "__get__portfolio",
        "__create__portfolio"
      ]
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findOne"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__portfolio_coins"
    }
  ],
  "methods": {}
}

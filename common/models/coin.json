{
  "name": "Coin",
  "description": "A model for Coins (& assets)",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "symbol": {
      "type": "string",
      "required": true
    },
    "rank": {
      "type": "string"
    },
    "algorithm": {
      "type": "string"
    },
    "totalCoins": {
      "type": "number"
    },
    "sourceCode": {
      "type": "string"
    },
    "explorer": {
      "type": "string"
    },
    "website": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "populate": {
      "accepts": [
        {
          "arg": "doit",
          "type": "boolean",
          "required": true,
          "description": "Tell me whether to populate the db or not"
        }
      ],
      "returns": [
        {
          "arg": "doit",
          "type": "object",
          "root": false,
          "description": "The Response of the populate method"
        }
      ],
      "description": "Takes in true or false",
      "http": [
        {
          "path": "/populate/:doit",
          "verb": "post"
        }
      ]
    }
  }
}
